{
  "name": "ceviz",
  "type": "module",
  "version": "0.0.3",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "author": "",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "pnpm --filter ceviz build",
    "dev": "pnpm --filter ceviz dev",
    "analyze": "pnpm --filter ceviz build && node packages/core/dist/cli.mjs analyze",
    "test": "pnpm --filter ceviz test",
    "bumpp": "bumpp package.json packages/*/package.json",
    "release": "pnpm bumpp && cd packages/core && pnpm pu:release",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.1"
  }
}
